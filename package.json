{
  "name": "babel-preset-proposal-typescript",
  "version": "2.0.3",
  "description": "Yet another Babel preset for TypeScript, only transforms proposals which TypeScript does not support now.",
  "repository": "git+https://github.com/rx-ts/babel-preset-proposal-typescript.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=6.9.0"
  },
  "main": "lib/cjs",
  "module": "lib",
  "es2015": "lib/es2015",
  "fesm5": "lib/esm",
  "types": "lib",
  "files": [
    "lib",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "babel-preset",
    "babel-preset-ts",
    "babel-preset-typescript",
    "@babel/preset-ts",
    "@babel/preset-typescript",
    "babel-typescript",
    "typescript"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r",
    "build:ts": "tsc -P src",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts -f friendly",
    "lint:tsc": "tsc -P src --noEmit",
    "prepare": "simple-git-hooks && yarn-deduplicate --strategy fewer || exit 0",
    "prerelease": "npm run build",
    "pretest": "yarn build:r",
    "release": "clean-publish && changeset publish",
    "test": "jest"
  },
  "peerDependencies": {
    "@babel/core": "^7.14.1",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.13.0",
    "@babel/plugin-proposal-async-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-class-static-block": "^7.13.11",
    "@babel/plugin-proposal-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-proposal-partial-application": "^7.12.13",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.13",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
    "@babel/plugin-proposal-record-and-tuple": "^7.13.0",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-decorators": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-typescript": "^7.12.13",
    "@bloomberg/record-tuple-polyfill": "^0.0.3",
    "compare-versions": "^3.6.0"
  },
  "devDependencies": {
    "@1stg/lib-config": "^1.2.10",
    "@babel/eslint-plugin": "^7.13.16",
    "@changesets/cli": "^2.16.0",
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "clean-publish": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "babel-preset-proposal-typescript": "link:.",
    "prettier": "^2.3.0"
  }
}
